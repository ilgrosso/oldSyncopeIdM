<dataset>
    <SyncopeConfiguration confKey="token.length" confValue="256"/>
    <SyncopeConfiguration confKey="token.expireTime" confValue="60"/>
    <SyncopeConfiguration confKey="smtp.host" confValue="smtp.ngi.it"/>
    <SyncopeConfiguration confKey="activate.email.from" confValue="syncope@googlecode.com"/>
    <SyncopeConfiguration confKey="activate.email.subject" confValue="Welcome to Syncope!"/>
    <SyncopeConfiguration confKey="activate.email.template.html" confValue="registration.html"/>
    <SyncopeConfiguration confKey="activate.email.template.txt" confValue="registration.txt"/>
    <SyncopeConfiguration confKey="identityconnectors.bundle.directory"
                          confValue="/tmp/bundles"/>
                          
    <SyncopeUser id="1" password="XXX"/>
    <SyncopeUser id="2" password="YYY"/>
    <SyncopeUser id="3" password="ZZZ"/>
    <SyncopeUser id="4" password="PPP"/>

    <SyncopeRole id="1" name="root"/>
    <SyncopeRole id="2" name="child" parent="root"/>
    <SyncopeRole id="3" name="citizen"/>
    <SyncopeRole id="4" name="employee" parent="citizen"/>
    <SyncopeRole id="5" name="secretary" parent="employee"/>
    <SyncopeRole id="6" name="director" parent="employee"/>
    <SyncopeRole id="7" name="managingDirector" parent="director"/>

    <SyncopeUser_SyncopeRole users_id="1" roles_id="1"/>
    <SyncopeUser_SyncopeRole users_id="2" roles_id="1"/>
    <SyncopeUser_SyncopeRole users_id="2" roles_id="2"/>
    <SyncopeUser_SyncopeRole users_id="4" roles_id="7"/>

    <UserSchema name="username" type="String" virtual="false"
                mandatory="true" multivalue="false" uniquevalue="true"/>
    <UserSchema name="userId" type="String" virtual="false"
                mandatory="true" multivalue="false" uniquevalue="true"
                validatorClass="org.syncope.core.persistence.validation.EmailAddressValidator"/>
    <UserSchema name="loginDate" type="Date" virtual="false"
                mandatory="false" multivalue="true" uniquevalue="false"
                conversionPattern="yyyy-MM-dd"/>
    <UserSchema name="firstname" type="String" virtual="false"
                mandatory="false" multivalue="false" uniquevalue="false"/>
    <UserSchema name="surname" type="String" virtual="false"
                mandatory="true" multivalue="false" uniquevalue="false"/>
    <UserSchema name="email" type="String" virtual="false"
                mandatory="false" multivalue="true" uniquevalue="true"
                validatorClass="org.syncope.core.persistence.validation.EmailAddressValidator"/>
    <UserSchema name="activationDate" type="Date" virtual="false"
                mandatory="false" multivalue="false" uniquevalue="false"
                conversionPattern="yyyy-MM-dd'T'HH:mm:ss.SSSZ"/>
                
    <RoleSchema name="icon" type="String" virtual="false"
                mandatory="false" multivalue="false" uniquevalue="false"/>
    <RoleSchema name="show" type="Boolean" virtual="false"
                mandatory="false" multivalue="false" uniquevalue="false"/>

    <UserAttribute id="100" owner_id="1" schema_name="username"/>
    <UserAttribute id="200" owner_id="1" schema_name="loginDate"/>
    <UserAttribute id="300" owner_id="2" schema_name="username"/>
    <UserAttribute id="400" owner_id="3" schema_name="firstname"/>
    <UserAttribute id="500" owner_id="3" schema_name="surname"/>
    <UserAttribute id="550" owner_id="4" schema_name="loginDate"/>

    <RoleAttribute id="600" owner_id="1" schema_name="icon"/>
    <RoleAttribute id="700" owner_id="2" schema_name="icon"/>
    <RoleAttribute id="800" owner_id="1" schema_name="show"/>

    <UserAttributeValue attribute_id="100" id="10" stringValue="chicchiricco"/>
    <UserAttributeValue attribute_id="300" id="11" stringValue="fabio.martelli"/>
    <UserAttributeValue attribute_id="200" id="20" dateValue="2009-05-26"/>
    <UserAttributeValue attribute_id="200" id="21" dateValue="2010-05-26 15:40:04"/>
    <UserAttributeValue attribute_id="400" id="30" stringValue="John"/>
    <UserAttributeValue attribute_id="500" id="31" stringValue="Doe"/>
    <UserAttributeValue attribute_id="550" id="22" dateValue="2009-06-24"/>

    <RoleAttributeValue attribute_id="600" id="40" stringValue="niceIcon"/>
    <RoleAttributeValue attribute_id="700" id="41" stringValue="badIcon"/>
    <RoleAttributeValue attribute_id="800" id="42" booleanValue="TRUE"/>

    <UserDerivedSchema name="cn" expression="surname + ', ' + firstname"/>
    
    <UserDerivedSchema_UserSchema derivedSchemas_name="cn"
                                  schemas_name="surname" />
    <UserDerivedSchema_UserSchema derivedSchemas_name="cn"
                                  schemas_name="firstname" />
                                            
    <UserDerivedAttribute id="1000" derivedSchema_name="cn" owner_id="3"/>

    <ConnectorInstance id="100"
                       bundleName="org.syncope.identityconnectors.bundles.staticws"
                       connectorName="org.syncope.identityconnectors.bundles.staticwebservice.WebServiceConnector"
                       version="0.1-SNAPSHOT"
                       xmlConfiguration="%3C%3Fxml+version%3D%221.0%22+encoding%3D%22UTF-8%22%3F%3E+%0A%3Cjava+version%3D%221.6.0_20%22+class%3D%22java.beans.XMLDecoder%22%3E+%0A+%3Cobject+class%3D%22java.util.HashSet%22%3E+%0A++%3Cvoid+method%3D%22add%22%3E+%0A+++%3Cobject+class%3D%22org.syncope.client.to.PropertyTO%22%3E+%0A++++%3Cvoid+property%3D%22key%22%3E+%0A+++++%3Cstring%3Eendpoint%3C%2Fstring%3E+%0A++++%3C%2Fvoid%3E+%0A++++%3Cvoid+property%3D%22value%22%3E+%0A+++++%3Cstring%3Ehttp%3A%2F%2Flocalhost%3A8888%2Fwstarget%2Fservices%3C%2Fstring%3E+%0A++++%3C%2Fvoid%3E+%0A+++%3C%2Fobject%3E+%0A++%3C%2Fvoid%3E+%0A++%3Cvoid+method%3D%22add%22%3E+%0A+++%3Cobject+class%3D%22org.syncope.client.to.PropertyTO%22%3E+%0A++++%3Cvoid+property%3D%22key%22%3E+%0A+++++%3Cstring%3Eservicename%3C%2Fstring%3E+%0A++++%3C%2Fvoid%3E+%0A++++%3Cvoid+property%3D%22value%22%3E+%0A+++++%3Cstring%3EProvisioning%3C%2Fstring%3E+%0A++++%3C%2Fvoid%3E+%0A+++%3C%2Fobject%3E+%0A++%3C%2Fvoid%3E+%0A+%3C%2Fobject%3E+%0A%3C%2Fjava%3E+%0A" />

    <Resource name="ws-target-resource-1" connector_id="100"/>
    <Resource name="ws-target-resource-2" connector_id="100"/>
    <Resource name="ws-target-resource-list-mappings-1" connector_id="100"/>
    <Resource name="ws-target-resource-list-mappings-2" connector_id="100"/>

    <Resource_SyncopeRole Resource_name="ws-target-resource-list-mappings-1" roles_id="3" />
    <SyncopeRole_Resource SyncopeRole_id="3" resources_name="ws-target-resource-list-mappings-1" />

    <Resource_SyncopeUser Resource_name="ws-target-resource-2" users_id="3" />
    <SyncopeUser_Resource SyncopeUser_id="3" resources_name="ws-target-resource-2" />

    <Resource_SyncopeRole Resource_name="ws-target-resource-list-mappings-2" roles_id="3" />
    <SyncopeRole_Resource SyncopeRole_id="3" resources_name="ws-target-resource-list-mappings-2" />

    <SchemaMapping id="100" field="username"
                   resource_name="ws-target-resource-1"
                   userSchema_name="email" nullable="false"
                   accountid="true" password="false"/>

    <SchemaMapping id="101" field="username"
                   resource_name="ws-target-resource-2"
                   userSchema_name="email" nullable="false"
                   accountid="true" password="false"/>

    <SchemaMapping id="102" field="email"
                   resource_name="ws-target-resource-list-mappings-1"
                   userSchema_name="email" nullable="false"
                   accountid="true" password="false"/>

    <SchemaMapping id="103" field="firstname"
                   resource_name="ws-target-resource-list-mappings-1"
                   userSchema_name="email" nullable="false"
                   accountid="true" password="false"/>

    <SchemaMapping id="104" field="firstname"
                   resource_name="ws-target-resource-list-mappings-2"
                   userSchema_name="email" nullable="false"
                   accountid="true" password="false"/>

    <Entitlement name="base"/>
    <Entitlement name="advanced" description="Advanced entitlement"/>

    <SyncopeRole_Entitlement entitlements_name="base"
                             roles_id="1"/>
    <SyncopeRole_Entitlement entitlements_name="advanced"
                             roles_id="1"/>
    <SyncopeRole_Entitlement entitlements_name="base"
                             roles_id="2"/>

    <RoleDerivedSchema name="displayProperty"
                       expression="icon + ': ' + show"/>

    <RoleDerivedSchema_RoleSchema derivedSchemas_name="displayProperty"
                                  schemas_name="icon" />
    <RoleDerivedSchema_RoleSchema derivedSchemas_name="displayProperty"
                                  schemas_name="show" />

    <RoleDerivedAttribute id="1001" derivedSchema_name="displayProperty"
                          owner_id="1"/>

</dataset>
